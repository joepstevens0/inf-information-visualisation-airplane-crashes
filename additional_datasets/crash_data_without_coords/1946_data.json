[{"date": "January 06, 1946", "time": "03", "location": "Birmingham, Alabama", "operator": "Pennsylvania Central Airlines", "flight#": "105", "route": "New York City - Birmingham, AL", "actype": "Douglas DC-3", "registration": "N21786", "cn/ln": "4131", "aboard": "11", "aboard_passengers": "7", "aboard_crew": "4", "fatalities": "3", "fatalities_passengers": "0", "fatalities_crew": "3", "ground": "0", "summary": "In worsening weather conditions, the plane landed fast and high while making an instrument approach to Birmingham, overran the runway, crashed through the barrier fence and into a creek. The action of the pilot in committing himself to a landing from an approach which was too high and too fast."}, {"date": "January 09, 1946", "time": "?", "location": "Changchun, China", "operator": "Military - Chinese Air Force", "flight#": "?", "route": "?", "actype": "Douglas C-47", "registration": "?", "cn/ln": "?", "aboard": "18", "aboard_passengers": "?", "aboard_crew": "?", "fatalities": "18", "fatalities_passengers": "?", "fatalities_crew": "?", "ground": "0", "summary": "?"}, {"date": "January 14, 1946", "time": "?", "location": "Near Netrubezh, Russia", "operator": "Aeroflot", "flight#": "?", "route": "Kharkov, Ukraine - Moscow", "actype": "Lisunov Li-2", "registration": "SSSR-4150", "cn/ln": "?", "aboard": "22", "aboard_passengers": "17", "aboard_crew": "5", "fatalities": "22", "fatalities_passengers": "17", "fatalities_crew": "5", "ground": "0", "summary": "Experienced severe icing resulting in buffeting and structural failure."}, {"date": "January 15, 1946", "time": "18", "location": "Marseilles, France", "operator": "Military -Royal Air Force", "flight#": "?", "route": "?", "actype": "Douglas C-47 Dakota", "registration": "KN557", "cn/ln": "33171", "aboard": "25", "aboard_passengers": "21", "aboard_crew": "4", "fatalities": "23", "fatalities_passengers": "19", "fatalities_crew": "4", "ground": "0", "summary": "Crashed into a mountainside in a snowstorm."}, {"dat